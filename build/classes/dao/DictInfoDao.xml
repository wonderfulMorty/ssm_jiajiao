<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiajiao.dao.DictInfoDao">

    <select id="findByCode" resultType="com.jiajiao.bean.DictInfo">
     SELECT dictid dictId,`code`,content,linkDict  FROM t_dictinfo WHERE `code` =#{0}
    </select>
    
    
	
    
    
</mapper>